unit LastLoop;

interface
uses FirstLoop, Base, vcl.graphics;
type

  TLastLoop = class(TFirstLoop)
  public
    constructor Create(AYStart, AXStart, AXLast: Integer; const AAction: string;
                       const ABaseBlock: TBlock; const ACanvas: TCanvas); override;
  end;

implementation

  constructor TLastLoop.Create(AYStart, AXStart, AXLast: Integer; const AAction: string;
                               const ABaseBlock: TBlock; const ACanvas: TCanvas);
  begin
    inherited Create(AYStart + TBlock.DefaultBlockSize, AXStart,
                     AXLast, AAction, ABaseBlock, ACanvas);
  end;

end.
